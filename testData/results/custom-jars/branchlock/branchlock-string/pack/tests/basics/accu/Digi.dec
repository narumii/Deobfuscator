package pack.tests.basics.accu;

public class Digi {
    public void run() {
        double fl = 0.0;
        int co = 0;
        float fx = 1.1F;

        do {
            fl += 1.0E-18;
        } while (++co <= 100 && (float)fl != 2.0E-17F);

        if (co == 20) {
            fx = 2.4F;
            if (true) {
                System.out.println("PASS");
                return;
            }

            System.out.println("FAIL");
        } else {
            System.out.println("FAIL");
        }
    }
}
